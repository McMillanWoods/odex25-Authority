#. module: helpdesk_complain
#: model:mail.template,body_html:helpdesk_complain.ticket_complain_email_template
msgid ""
"<?xml version=\"1.0\"?>\n"
"<table style=\"width:600px;margin:5px auto;direction:initial;\">\n"
"                <tbody>\n"
"                    <tr>\n"
"                        <td style=\"text-align:left;\">\n"
"                            Dear ${object.team_id.team_leader_id.name or 'Madam, Sir'},\n"
"                        </td>\n"
"                    </tr>\n"
"                    <tr>\n"
"                        <td style=\"text-align:left;\">\n"
"                            The Ticket\n"
"                            % if object.access_token:\n"
"                            <a href=\"/helpdesk/ticket/${object.id}/${object.access_token}\">${object.name}</a>\n"
"                            % endif\n"
"                            that assigned to ${(object.user_id.name or '')} have received a complain from the customer, please review the ticket.\n"
"                        </td>\n"
"                    </tr>\n"
"                    <tr>\n"
"                        <td style=\"text-align:left;\">\n"
"                            The reference of the ticket is ${object.id}.\n"
"                        </td>\n"
"                    </tr>\n"
"                    <tr style=\"direction:rtl;\" dir=\"rtl\">\n"
"                        <td style=\"text-align:right;\">\n"
"                            عزيزي ${object.team_id.team_leader_id.name or 'سيدي، سيدتي'}\n"
"                        </td>\n"
"                    </tr>\n"
"                    <tr style=\"direction:rtl;\" dir=\"rtl\">\n"
"                        <td style=\"text-align:right;direction:rtl;\" dir=\"rtl\">\n"
"                            التذكره\n"
"                            % if object.access_token:\n"
"                            <a href=\"/helpdesk/ticket/${object.id}/${object.access_token}\">${object.name}</a>\n"
"                            % endif\n"
"                            المسندة إلى ${(object.user_id.name or '')} تم رفع شكوى عليها من العميل، الرجاء مراجعة التذكرة\n"
"                        </td>\n"
"                    </tr>\n"
"                    <tr style=\"direction:rtl;\" dir=\"rtl\">\n"
"                        <td style=\"text-align:right;\">\n"
"                            الرقم المرجعي للتذكرة ${object.id}.\n"
"                        </td>\n"
"                    </tr>\n"
"                </tbody>\n"
"            </table>\n"
"        "
msgstr ""

