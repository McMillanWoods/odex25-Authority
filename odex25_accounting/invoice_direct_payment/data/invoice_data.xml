<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="invoice_sequence" model="ir.sequence">
            <field name="name">Invoice Sequence</field>
            <field name="code">invoice.seq</field>
            <field name="prefix">EO/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">6</field>
        </record>
        <record id="request_sequence" model="ir.sequence">
            <field name="name">FR Sequence</field>
            <field name="code">request.seq</field>
            <field name="prefix">FR/%(range_year)s/</field>
            <field eval="True" name="use_date_range"/>
            <field name="padding">6</field>
        </record>
    </data>
</odoo>